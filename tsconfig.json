{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "checkJs": true,
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
  }
}
